<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="noticeconfig" namespace="/admin/notice"
		extends="wzd-admin">
		<action name="noticeConfigList" class="noticeConfigAction" method="noticeConfigList">
			<result name="success" type="ftl">/admin/notice/noticeConfigList.html
			</result>
		</action>
		<action name="update" class="noticeConfigAction" method="update">
			<result name="success" type="ftl">/admin/system/index.html
			</result>
		</action>

		<action name="addNoticeConfig" class="noticeConfigAction" method="addNoticeConfig">
			<result name="success" type="ftl">/admin/notice/addNoticeConfig.html
			</result>
		</action>
	</package>
</struts>
